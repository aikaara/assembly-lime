{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "bun run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://assembly-line-ukroi.ondigitalocean.app/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
